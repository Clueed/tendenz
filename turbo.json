{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "typecheck": {},
    "build": {
      "dependsOn": ["db:generate"]
    },
    "lint": {},
    "format": {},
    "pre-commit": {
      "dependsOn": ["typecheck", "lint", "format"]
    },
    "dev": {
      "dependsOn": ["db:generate"],
      "cache": false,
      "persistent": false
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  }
}
