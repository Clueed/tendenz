// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model UsStockDaily {
  UsStocks       UsStocks @relation(fields: [ticker], references: [ticker])
  ticker         String
  date           DateTime
  volume         Float
  open           Float
  close          Float
  high           Float
  low            Float
  volumeWeighted Float?
  nTransactions  Int?
  marketCap      Int?

  @@id([ticker, date])
}

model UsStocks {
  ticker String         @id
  name   String?
  Dailys UsStockDaily[]
}
